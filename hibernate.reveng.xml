<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="openproduct" match-name="resource"/>
  <table-filter match-catalog="openproduct" match-name="resource_has_rol"/>
  <table-filter match-catalog="openproduct" match-name="rol"/>
  <table-filter match-catalog="openproduct" match-name="user"/>
  <table-filter match-catalog="openproduct" match-name="user_has_rol"/>
</hibernate-reverse-engineering>